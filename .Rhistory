Institucion = c("ISET", "AyPC", "CPyAU", "CAyCC", "GENOMICAS", "CiSOC", "COMYCULT", "CREACION", "DERECHO", "FEHDI", "HISTYSOCCON", "SOFTWARE", "ISTU", "ISEI", "ISENER", "MODELACION", "NUTRICION", "PROMOCION", "PROTECCION"),
CH = c(4628, 30861, 3875, 6, 166, 588, 45230, 1966, 1805, 1151, 884, 618, 9672, 1772, 236, 142, 211, 30101, 126),
GAM = c(18211, 17043, 36315, 15, 119, 14410, 42077, 11903, 30905, 7957, 9658, 11688, 7889, 10348, 160, 827, 6585, 21931, 2600),
DV = c(93, 1065, 36016, 76, 5886, 206, 1413, 24891, 611, 12888, 2018, 84, 42, 166, 59, 61, 234, 1158, 16),
IZT = c(14183, 1988, 54457, 1465, 62, 588, 9207, 288, 1011, 421, 339, 550, 626, 6625, 205, 1888, 5028, 35379, 95),
SLT = c(36856, 41305, 116407, 2238, 460, 29077, 141630, 25837, 3095, 20571, 25619, 16902, 14664, 25524, 8701, 4698, 9378, 65514, 4584),
PESCER = c(29, 38, 1436, 0, 0, 0, 113, 383, 21794, 61, 0, 33, 5, 21, 0, 28, 0, 24, 0)
)
View(data_Lic)
library(ggplot2)
# Crear un gráfico para cada plantel
for (i in 2:ncol(data_Lic)) {  # Comenzamos en la segunda columna (CH)
plantel <- colnames(data_Lic)[i]  # Nombre del plantel
data <- data.frame(Institucion = data_Lic$Institucion, Valor = data_Lic[, i])
# Crear el gráfico
gg <- ggplot(data, aes(x = Institucion, y = Valor)) +
geom_line() +
labs(
title = paste("Valores para", plantel),
x = "Institución",
y = "Valor"
) +
theme_minimal() +
theme(axis.text.x = element_text(angle = 90, hjust = 1))
# Mostrar el gráfico
print(gg)
}
library(ggplot2)
# Crear un gráfico de barras para cada plantel
for (i in 2:ncol(data_Lic)) {  # Comenzamos en la segunda columna (CH)
plantel <- colnames(data_Lic)[i]  # Nombre del plantel
data <- data.frame(Institucion = data_Lic$Institucion, Valor = data_Lic[, i])
# Crear el gráfico de barras
gg <- ggplot(data, aes(x = Institucion, y = Valor)) +
geom_bar(stat = "identity", fill = "blue") +
labs(
title = paste("Valores para", plantel),
x = "Institución",
y = "Valor"
) +
theme_minimal() +
theme(axis.text.x = element_text(angle = 90, hjust = 1))
# Mostrar el gráfico
print(gg)
}

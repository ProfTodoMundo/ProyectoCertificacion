library(magick)
# Establecer la ruta completa a la carpeta que contiene los archivos PDF
carpeta_pdf <- "~/Desktop/MiGithub/ProyectoCertificacion/Graficas"
# Obtener la lista de archivos PDF en la carpeta
archivos_pdf <- list.files(carpeta_pdf, pattern = ".pdf", full.names = TRUE)
# Función para convertir un archivo PDF a PNG
convertir_pdf_a_png <- function(ruta_pdf) {
# Obtener el nombre del archivo sin extensión
nombre_base <- tools::file_path_sans_ext(basename(ruta_pdf))
# Obtener la ruta de salida para el archivo PNG (misma carpeta que el PDF)
nombre_salida <- file.path(dirname(ruta_pdf), paste0(nombre_base, ".png"))
# Convertir el PDF a una imagen PNG
pdf_convert(ruta_pdf, format = "png", pages = 1, filenames = nombre_salida)
}
# Recorrer la lista de archivos PDF y convertirlos a PNG
for (pdf in archivos_pdf) {
convertir_pdf_a_png(pdf)
}
